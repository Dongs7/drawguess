extends layout

block append link
  script(src='https://cdn.socket.io/socket.io-1.3.5.js')
  script(src='http://code.jquery.com/jquery-1.11.1.js')
  script(src=prefix+'/javascripts/test.js')

block content
  div(id='infobox')
    div(id='room')=room
    div(id='cookie')
      div(id='guest')= user.guest
      - if(!user.guest){
        div(id='id')= user.id
        div(id='username')= user.username
        div(id='nickname')= user.nickname
      - }
  h2 there's 
    span(id='count') 0
    span in this room
    
  h2 game status:
    span(id='status')
  
  h2 the answer is:
    span(id='answer')
    
  h2 Timer:
    span(id='timer')
    
  
  ul(id='list')
    li default things
    
  form(action='')
    input(class='msg', id='msg')
    button(type='submit') Enter