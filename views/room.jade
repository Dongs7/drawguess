extends layout

block append link
  script(src='https://cdn.socket.io/socket.io-1.3.5.js')
  script(src='http://code.jquery.com/jquery-1.11.1.js')
  script(src=prefix+'/javascripts/test.js')

block content
  div(id='infobox')
    div(id='room')=room
  
  h2 there's 
    span(id='count') 0 
    span in this room
   
  audio(id='player' loop='loop') 
   source(src='/bgmusic/1.wav' type='audio/mpeg')
  
  a(onclick="document.getElementById('player').play()")
    img(src='/images/on1.png')
  a(onclick="document.getElementById('player').pause()")
    img(src='/images/off2.png')

  h2 game status:
    span(id='status')
  
  h2 the answer is:
    span(id='answer')
    
  h2 Timer:
    span(id='timer')
    
  
  ul(id='list')
    li default things
    
  form(action='')
    input(class='msg', id='msg')
    button(type='submit') Enter
    
  //button(id='joingame') Join game
    
